  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "plugins": [
      "jest",
      "babel"
    ],
    "extends": [
      "airbnb",
      "prettier"
    ],
    "env": {
      "node": true,
      "jest": true
